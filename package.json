{
  "name": "hackernews-tegg",
  "description": "Hello Egg.js",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development npm run dev:base",
    "dev:base": "nodemon --watch src --exec \"ts-node --files -r tsconfig-paths/register --transpile-only ./src/app.ts\" --ext ts",
    "build": "rimraf dist && tsc --project tsconfig.prod.json",
    "start": "node dist/app.js",
    "prisma:generate": "prisma generate",
    "db:push:dev": "cross-env NODE_ENV=development prisma db push --force",
    "migration:dev": "cross-env NODE_ENV=development prisma migrate dev",
    "migration:prod:deploy": "cross-env NODE_ENV=production prisma migrate deploy"
  },
  "dependencies": {
    "@eggjs/scripts": "beta",
    "@eggjs/tracer": "beta",
    "egg": "beta"
  },
  "devDependencies": {
    "@eggjs/bin": "beta",
    "@eggjs/mock": "beta",
    "@eggjs/tsconfig": "beta",
    "@oxc-node/core": "^0.0.35",
    "@types/node": "24",
    "@vitest/coverage-v8": "4",
    "cross-env": "^10.1.0",
    "nodemon": "^3.1.11",
    "oxlint": "1",
    "oxlint-tsgolint": "^0.9.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "vitest": "4"
  },
  "engines": {
    "node": ">=22.18.0"
  },
  "homepage": "https://github.com/YOUR_USERNAME/YOUR_REPO#readme",
  "bugs": {
    "url": "https://github.com/YOUR_USERNAME/YOUR_REPO/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/YOUR_USERNAME/YOUR_REPO.git"
  },
  "author": "Author Name <author.name@mail.com>",
  "license": "MIT",
  "packageManager": "npm@10.9.2"
}
