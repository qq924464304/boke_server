{
  "name": "My Node.js & TypeScript App",
  "build": {
    "dockerfile": "../Dockerfile",
    "context": "..",
    "target": "base"
  },
  "workspaceFolder": "/app",
  "customizations": {
    "vscode": {
      "extensions": [
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode",
        "ms-vscode.vscode-typescript-tslint-plugin"
      ]
    }
  },
  "forwardPorts": [4000],
  "postCreateCommand": "npm install && npm run build",
  "remoteUser": "node"
}
